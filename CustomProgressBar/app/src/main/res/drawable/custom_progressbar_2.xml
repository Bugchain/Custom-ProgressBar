<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/progressbar_background" />
            <stroke android:color="@color/progressbar_stroke"
                android:width="@dimen/progressbar_stroke_width"/>
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/progressbar_progress" />
                <stroke android:color="@color/progressbar_stroke"
                    android:width="@dimen/progressbar_stroke_width"/>
            </shape>
        </clip>
    </item>
</layer-list>